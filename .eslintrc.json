{
  "env": {
    "es2021": true,
    "node": true
  },
  "extends": ["standard", "prettier"],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["@typescript-eslint", "prettier"],
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "singleQuote": true
      }
    ],
    "camelcase": "off",
    "no-use-before-define": "off",
    "no-unused-vars": "off",
    "no-useless-constructor": "off",
    "@typescript-eslint/no-unused-vars": ["error"]
  }
}
